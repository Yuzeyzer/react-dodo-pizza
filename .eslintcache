\r\nimport PizzaItem from '../components/pizzaItem';\r\nimport Item from '../components/item';\r\nimport Sort from '../components/sort';\r\nimport Categories from '../components/categories';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport { addPizzaToCart } from '../redux/actions/cart';\r\n\r\nconst Home = () => {\r\n  const [pizzas, setPizzas] = React.useState([]);\r\n  const [snacks, setSnacks] = React.useState([]);\r\n  const [search, setSearch] = React.useState('');\r\n  const dispatch = useDispatch();\r\n\r\n  React.useEffect(() => {\r\n    fetch('http://localhost:3000/db.json')\r\n      .then((response) => response.json())\r\n      .then(({ pizzas, snacks }) => {\r\n        setSnacks(snacks);\r\n        setPizzas(pizzas);\r\n      });\r\n  }, []);\r\n\r\n  const handleSearchValue = (event) => setSearch(event.target.value);\r\n  const handleAddPizzaToCart = (obj) => dispatch(addPizzaToCart(obj));\r\n\r\n  return (\r\n    <div className='content'>\r\n      <div className='container'>\r\n        <div className='content__top'>\r\n          <Categories items={['Все', 'Мясные', 'Вегетарианская', 'Гриль', 'Острые', 'Закрытые']} />\r\n          <Sort />\r\n        </div>\r\n        <input\r\n          type='text'\r\n          placeholder='Введите имя для поиска'\r\n          onChange={(e) => {\r\n            handleSearchValue(e);\r\n          }}\r\n        />\r\n        <h2 className='content__title'>Все пиццы</h2>\r\n        <div className='content__items'>\r\n          {pizzas\r\n            .filter((item) => {\r\n              if (search === '') {\r\n                return item;\r\n              } else if (item.name.toLocaleLowerCase().includes(search.toLocaleLowerCase())) {\r\n                return item;\r\n              }\r\n            })\r\n            .map((item) => {\r\n              return <PizzaItem onClickAddPizza={handleAddPizzaToCart} key={item.id} {...item} />;\r\n            })}\r\n        </div>\r\n        <h2 className='content__title'>Закуски</h2>\r\n        <div className='content__items'>\r\n          {snacks\r\n            .filter((item) => {\r\n              if (search === '') {\r\n                return item;\r\n              } else if (item.name.toLocaleLowerCase().includes(search.toLocaleLowerCase())) {\r\n                return item;\r\n              }\r\n            })\r\n            .map((item) => {\r\n              return <Item key={item.id} {...item} />;\r\n            })}\r\n        </div>\r\n        <h2 className='content__title'>Десерты</h2>\r\n        <div className='content__items'>\r\n          {snacks\r\n            .filter((item) => {\r\n              if (search === '') {\r\n                return item;\r\n              } else if (item.name.toLocaleLowerCase().includes(search.toLocaleLowerCase())) {\r\n                return item;\r\n              }\r\n            })\r\n            .map((item) => {\r\n              return <Item key={item.id} {...item} />;\r\n            })}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\header\\index.js",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\pages\\cart.jsx",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\pizzaItem\\index.js",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\sort\\index.js",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\Categories\\index.js",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\categories\\index.js",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\pizzaItem\\types.jsx",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\pizzaItem\\sizes.jsx",[],["87","88"],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\\src\\components\\cartItem\\index.js",[],"C:\\Users\\кибер\\Desktop\\JS-10-13\\react-dodo-pizza\